	color: #fff;
	padding: 2px 5px;
}

.article_floor_con .article_floor_con_show {
	position: relative;
	min-height: 262px;
	border: 1px solid #ddd;
	padding: 15px;
	box-shadow: 1px 2px 3px #aaa;
	background-color: #fff;
	margin-bottom: 20px;
}

.article_floor_con .article_floor_con_show .title {
	width: 100%;
	font-weight: bold;
}

.article_floor_con .article_floor_con_show .content {
	padding: 5px 0;
}

.article_floor_con .article_floor_con_show .bottom_text {
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	padding: 5px 0;
	background-color: #eee;
	font-weight: bold;
	font-size: 12px;
}

.article_floor_con .article_floor_con_show .bottom_text span.title {
	margin-left: 15px;
}

#mylist {
	display: none;
	max-height: 100px;
	border: 1px solid #ccc;
	overflow-y: auto;
	/* 防止水平滚动条 */
	overflow-x: hidden;
	background-color: #fff;
	z-index: 9999;
}

#mylist {
	list-style: none;
	padding-left: 0;
}

#mylist li {
	padding-left: 10px;
}

#mylist li:hover,
#mylist li.hover {
	background-color: #676A6C;
	color: #fff;
}

#runModel {
	display: inline-block;
	font-size: 12px;
	margin-left: 3px;
	margin-right: 10px;
	color: #AA0000;
	text-decoration: none;
}

#fixedBox {
	position: absolute;
	z-index: 999;
	display: none;
}

#fixedBox .fixedBox_re {
	position: relative;
}

#fixedBox .arrow {
	position: absolute;
	background-repeat: no-repeat;
}

#fixedBox .arrow.top {
	background: url("../img/ts-t.png");
	top: -8px;
	left: 50%;
	width: 12px;
	height: 8px;
	margin-left: -6px;
}

#fixedBox .arrow.bottom {
	background: url("../img/ts-d.png");
	bottom: -8px;
	left: 50%;
	width: 12px;
	height: 8px;
	margin-left: -6px;
}

#fixedBox .arrow.left {
	background: url("../img/ts-l.png");
	top: 5px;
    left: -8px;
    width: 8px;
    height: 12px;
}

#fixedBox .arrow.right {
	background: url("../img/ts-r.png");
	top: 5px;
    right: -8px;
    width: 8px;
    height: 12px;
}

#fixedBox .content {
	display: inline-block;
	background-color: #19e71e;
	font-size: 12px;
	color: #333333;
	text-align: center;
	padding: 3px 8px;
	border-radius: 5px;
}

#example-navbar-collapse{
	position: absolute;
	right: 5px;
	top: 35px;
	background-color: #fff;
	z-index: 999;
	display: none !important;
	overflow: hidden;
}

#example-navbar-collapse li a{
	border: 1px solid #eee;
}


/*蒙板*/

.mask-box {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 10;
	width: 100%;
	height: 100%;
	background: url("../img/banner2.png") no-repeat;
	background-size: 100%;
}


/* Media */

@media screen and (max-width: 768px) {
	.aside {
		display: none;
	}
	/*.runMode_span {
		display: none;
	}*/
	.article {
		padding: 0;
	}
	.article_floor_con .section {
		margin-bottom: 20px;
	}
	.article .article_floor_con .article_abs span.apSignal {
		display: inline-block;
		width: 100%;
	}
	.article .article_top_right {
		width: 100%;
	}
	.article .article_top_right #QRCode {
		display: inline-block;
	}
	.article .article_floor .article_abs_all_1 div {
		display: block;
	}
	#example-navbar-collapse.active{
		display: inline-block !important;
	}
}

@media screen and (max-width: 321px) {
	.article .article_top p {
		width: 60px;
		white-space: nowrap;
		overflow: hidden;
	}
}


