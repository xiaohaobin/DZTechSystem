html,
body {
	height: 100%;
}

body {
	font-family: 微软雅黑, "microsoft yahei", tahoma, helvetica, arial;
}

img {
	width: 100%;
}

ul {
	list-style: none;
}

body>.content {
	width: 100%;
	height: 100%;
	background-color: #F3F3F4;
	color: #676A6C;
}

.aside {
	position: fixed;
	left: 0;
	top: 0;
	width: 45px;
	height: 100%;
	background-color: #2F4050;
	text-align: center;
	z-index: 500;
}

.aside div {
	position: relative;
}

.aside div.aside_li a {
	display: inline-block;
	width: 100%;
	height: 40px;
	line-height: 45px;
}

.aside div.aside_li:hover {
	background-color: #394957;
}

.aside div.aside_li.active {
	background-color: #283846;
}

.aside div.aside_li:hover:before,
.aside div.aside_li.active:before {
	top: 0;
	bottom: 0;
}

.aside div.aside_li:before {
	content: "";
	position: absolute;
	left: 0;
	top: 50%;
	bottom: 50%;
	width: 4px;
	background: #1C84C6;
	-webkit-transition-property: 'top,bottom,background-color';
	transition-property: 'top,bottom,background-color';
	-webkit-transition-duration: .3s;
	transition-duration: .3s;
	-webkit-transition-timing-function: ease-out;
	transition-timing-function: ease-out;
}

.aside div.aside_logo {
	padding: 8px;
	margin-bottom: 10px;
	line-height: normal;
}

.aside span {
	color: #fff;
	font-size: 18px;
}

.article {
	width: 100%;
	padding-left: 45px;
}

.article .article_top {
	position: relative;
	width: 100%;
	height: 45px;
	line-height: 45px;
	background-color: #fff;
	box-shadow: 1px 2px 3px #ccc;
}

.article .article_top p {
	float: left;
	margin-left: 25px;
}

.article .article_top span.version {
	margin-left: 10px;
	color: #0a0;
	font-size: 12px;
}

.article .article_top_right {
	position: absolute;
	top: 0;
	right: 10px;
	width: 50%;
	background-color: #fff;
}

.article .article_top_right span {
	font-size: 18px;
	color: #676A6C;
	float: right;
	line-height: 40px;
	margin-right: 12px;
}

.article .article_top_right #QRCode {
	float: right;
	font-size: 12px;
}

.article .article_top_right #menuBtn {
	float: right;
	margin-left: 10px;
	font-size: 20px;
	color: #676a6c;
	padding: 0;
	margin-right: 0;
}

#queryState {
	margin-top: 15px;
	text-align: center;
	color: #a00;
}

#queryState .glyphicon {
	position: inherit;
}

.article .isNumber_div {
	float: right;
	background-color: #eee;
	border: 1px solid #ccc;
	width: 60%;
	min-width: 120px;
	height: 26px;
	line-height: 26px;
	border-radius: 12px;
	margin-top: 9px;
	position: relative;
}

.article #isNumber {
	width: 100%;
	border: 0;
	padding: 0 25% 5px 15px;
	outline: 0;
	background: transparent;
	font-size: 12px;
}

.article #isSelect {
	display: inline-block;
	width: 25%;
	height: 24px;
	line-height: 24px;
	text-align: center;
	background: transparent;
	border: 0;
	border-left: 1px solid #ccc;
	color: #676A6C;
	outline: 0;
	padding: 0;
	font-size: 13px;
	position: absolute;
	top: 0;
	right: 0;
}

.article .article_floor {
	width: auto;
	min-height: 100px;
	margin: 12px 10px 0px;
	border-top: 4px solid #E7EAEC;
	background-color: #fff;
	box-shadow: 1px 2px 1px #ccc;
}

.article .article_floor_top {
	width: 100%;
	height: 30px;
	line-height: 30px;
	margin-bottom: 1px;
	border-bottom: 1px solid #F7F8F8;
}

.article .article_floor_top>p {
	display: inline-block;
	width: 15%;
	height: 30px;
	margin-left: 15px;
	font-weight: bold;
	font-size: 12px;
	overflow: hidden;
}

.article .article_floor_top div.article_floor_top_right {
	float: right;
	margin-right: 15px;
	font-weight: normal;
	font-size: 12px;
}

div.article_floor_top_right input#startdate {
	width: 75px;
	text-align: center;
	height: 20px;
	line-height: 20px;
	font-size: 12px;
}

.article .article_floor_con {
	position: relative;
	margin: 15px;
	padding-bottom: 10px;
}

.article .article_floor_con .article_abs {
	margin-bottom: 5px;
}

.article .article_floor_con .article_abs>span {
	margin-right: 15px;
}

.article .article_floor_con .article_abs>span>i {
	display: inline-block;
	width: 10px;
	height: 10px;
	border-radius: 50%;
	margin-right: 5px;
}

.article .article_floor_con .article_abs>span.apData>i {
	background-color: #fe0;
}

.article .article_floor_con .article_abs>span.rfData>i {
	background-color: #00dbdb;
}

.article .article_floor_con .article_abs>span.rfCrit>i {
	background-color: #e00;
}
.article .article_floor_con .article_abs>span.outCrit>i {
	background-color: #aaa;
}

.article .article_floor .article_abs_all {
	text-align: center;
	margin-top: 5px;
	font-size: 12px;
	font-weight: bold;
}

.article .article_floor .article_abs_all .article_abs_all_2 {
	/*display: none;*/
}
.article_abs_all_2,.article_abs_all_1{
	/*display: none;*/
}
.article .article_floor .article_abs_all_1 div {
	display: inline-block;
}

span#currentPop,
span#findSig {
	color: #a00;
	margin-right: 5px;
}

.article .article_floor_con ul.radar_ui {
	position: relative;
	height: 25px;
	font-size: 12px;
	font-weight: bold;
	padding-left: 0;
	border-bottom: 1px solid #ddd;
}

.article .article_floor_con ul.radar_ui li {
	display: none;
	margin: 0 5px;
	background-color: #fff;
}

.article .article_floor_con ul.radar_ui li.active {
	height: 25px;
	left: 0;
	top: 3px;
	display: inline-block;
}

.article .article_floor_con ul.radar_ui li.active a {
	color: #676A6C;
	background-color: #fff;
	border: 1px solid #ddd;
	border-bottom-color: transparent;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}

.article .article_floor_con ul.radar_ui li a {
	padding: 5px 12px;
	color: #676A6C;
}

.article_floor_con .section {
	padding: 0;
}

.article_floor_con .section_table {
	position: relative;
	width: 100%;
	height: 491px;
	overflow: auto;
}

.article_floor_con .section_table .section_table_error {
	display: block;
	font-weight: bold;
}

.article_floor_con .section_table .section_table_error p.status {
	text-align: center;
	color: #a00;
}

.section_left .quickChange{
	position: absolute;
	top: 0;
	right: 0;
	z-index: 500;
}

.section_left button{
	border: 1px solid #aaa;
	background-color: #fff;
	outline: none;
	border-radius: 12px;
	width: 70px;
	height: 24px;
	font-size: 12px;
	margin-right: 10px;
}

.section_left button.active{
	background-color: #aa0000;
	color: #FFFFFF;
	border-color: #aa0000;
}

.section_left .tab-content .tab-pane {
	position: relative;
}

.section_left table {
	min-width: 500px;
	text-align: center;
	margin-bottom: 10px;
}

.section_left table th {
	text-align: center;
}
