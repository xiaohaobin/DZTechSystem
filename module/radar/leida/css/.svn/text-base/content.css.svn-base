.article .article_floor_con>ul.overview_ui {
	display: inline-block;
	width: 100%;
	padding: 0;
	border-bottom: 2px solid #F0F0F0;
}

.article_floor_con>ul.overview_ui li {
	text-align: center;
	color: #676A6C;
	padding: 20px;
	border-bottom: 1px solid #F0F0F0;
}

.article_floor_con>ul.overview_ui li div.p_num {
	position: relative;
	display: inline-block;
