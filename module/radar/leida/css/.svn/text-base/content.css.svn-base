	font-size: 25px;
	margin-bottom: 1px;
}

.article_floor_con>ul.overview_ui li p.p_title {
	font-size: 12px;
}

.article_floor_con>ul.overview_ui li p.p_abs {
	position: absolute;
	min-width: 35px;
	min-height: 20px;
	top: 0;
	right: -60px;
