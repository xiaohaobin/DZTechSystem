/*边框动画*/
 .borderAnimate{
	border: 2px solid rgb(0,15,225);
	border-radius:5px;
	cursor: pointer;
	animation:animateBorder 3s infinite linear;
	-moz-animation:animateBorder 3s infinite linear; /* Firefox */
	-webkit-animation:animateBorder 3s infinite linear; /* Safari and Chrome */
	-o-animation:animateBorder 3s infinite linear; /* Opera */
}
@keyframes animateBorder
{
	0%   {border-color:red;color: darkgreen;}
	25%  {border-color:yellow;color: blueviolet;}
	50%  {border-color:blue;color: yellow;}
	100% {border-color:green;color: red;}
}		       
@-moz-keyframes animateBorder
{
	0%   {border-color:red;color: darkgreen;}
	25%  {border-color:yellow;color: blueviolet;}
	50%  {border-color:blue;color: yellow;}
	100% {border-color:green;color: red;}
}		 
@-webkit-keyframes animateBorder
{
	0%   {border-color:red;color: darkgreen;}
	25%  {border-color:yellow;color: blueviolet;}
	50%  {border-color:blue;color: yellow;}
	100% {border-color:green;color: red;}
}		
@-o-keyframes animateBorder
{
	0%   {border-color:red;color: darkgreen;}
	25%  {border-color:yellow;color: blueviolet;}
	50%  {border-color:blue;color: yellow;}
	100% {border-color:green;color: red;}
}		