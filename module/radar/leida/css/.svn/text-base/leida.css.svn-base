.canvas_Box{
	position: relative;
	width: 100%;
	height: 100%;
	/*background-color: #333;*/
}

.canvas_Box:after{
	content: "";
	display: block;
	clear: both;
}

canvas{
	display: block;
}

#myCanvas1{
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 0;
}

#mySVG{
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 50;
}

#myCanvas2{
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 1;
}

#SweepData .article_abs{
	margin: 5px 0;
}

#RFData table {
    width: 100%;
